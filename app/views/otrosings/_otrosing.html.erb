  <tr id=<%="otrosing_#{otrosing.id}"%>>
        <td><%= link_to otrosing.id, otrosing_path(otrosing) %></td>
        <td><%= otrosing.actividad %></td>
        <td><%= otrosing.valor %></td>
        <td><%= otrosing.dia %></td>
        <td><%=l otrosing.created_at %></td>
        <td>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_otrosing_path(otrosing), remote: true, :class => 'btn btn-mini' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      otrosing_path(otrosing),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => 'btn btn-mini btn-danger' %>
        </td>
      </tr>
    