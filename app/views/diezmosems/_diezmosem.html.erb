   <tr id=<%="diezmosem_#{diezmosem.id}"%>>
      <tr>
        <td><%= link_to diezmosem.id, diezmosem_path(diezmosem) %></td>
        <td><%= diezmosem.valor %></td>
        <td><%= diezmosem.semana %></td>
        <td><%=l diezmosem.created_at %></td>
        <td>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_diezmosem_path(diezmosem), remote: true, :class => 'btn btn-mini' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      diezmosem_path(diezmosem),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => 'btn btn-mini btn-danger' %>
        </td>
      </tr>
    