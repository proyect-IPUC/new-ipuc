<%= simple_form_for(@diezmotri, :html => { :class => 'form-horizontal', remote: true  } ) do |f| %>
  <%= f.input :valor %>
  <%= f.input :trimestre %>
  <div class="form-actions">
    <%= f.button :submit, :class => 'btn-primary' %>
    <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                diezmotris_path, :class => 'btn' %>
  </div>
<% end %>
