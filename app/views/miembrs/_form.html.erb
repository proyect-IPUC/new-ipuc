<%= simple_form_for @miembr, :html => { :class => 'form-horizontal' } do |f| %>
  <%= f.input :nombre %>
  <%= f.input :apellido %>
  <%= f.input :cargo %>
  <%= f.input :celular %>
  <%= f.input :TelFijo %>
  <div class="form-actions">
    <%= f.button :submit, :class => 'btn-primary' %>
    <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                miembrs_path, :class => 'btn' %>
  </div>
<% end %>
