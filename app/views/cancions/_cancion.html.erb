<tr id=<%="cancion_#{cancion.id}"%>>
      <tr>
        <td><%= link_to cancion.id %></td>
        <td><%= cancion.cancion %></td>
        <td><%= cancion.genero %></td>
        <td><%= cancion.artista %></td>
        <td>
        <%= link_to t('.show', :default => t("helpers.links.Ver")),
                      cancion_path(cancion), remote: true, :class => 'btn btn-success'%>
                      <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_cancion_path(cancion), remote: true, :class => 'btn btn-mini' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      cancion_path(cancion),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => 'btn btn-mini btn-danger' %>
        </td>
        <td><%=l cancion.created_at %></td>
        
      </tr>
   